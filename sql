SELECT DISTINCT SATICI_NO FROM PAR_SAT --DISTINCT SATICI_NO AYNI OLANLAR GELMESİN--

SELECT * FROM PERSONEL ORDER BY BRUT ASC --ORDER BY ASC KUCUKTEN BUYUGE SIRALAR--

SELECT * FROM PERSONEL ORDER BY BRUT DESC --ORDER BY ASC BUYUKTEN KUCUGE SIRALAR--

SELECT * FROM PERSONEL ORDER BY AD ASC, SOYAD DESC --ORDER BY= AD KUCUKTEN BUYUGE SOYAD BUYUKTEN KUCUGE SIRALAR--

SELECT * FROM PERSONEL WHERE BRUT>5000 --BRUTU 5000 BUYUK OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE AD='AYŞE' --AD DEĞERİ AYŞE OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE DOG_TAR > '01-01-1960' --DOGUM TARIHI 01-01-1960 TAN BUUYUK OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE CINSIYET ='E' --CINSIYETI E OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BOL_NO=1 OR BOL_NO=5--BOLUM NO 1 YADA 5 OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BOL_NO=1 AND AD='ALİ'--BOLUM NO 1 VE AD ALİ OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE (BOL_NO=1 OR BOL_NO=5) AND CINSIYET='K'--BOLUM NO 1 YADA 5 OLANLAR VE KADINLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BOL_NO!=4 --BOLUM NO 4 OLMAYANLARI GETIRIR--
SELECT * FROM PERSONEL WHERE BOL_NO<>4 --BOLUM NO 4 OLMAYANLARI GETIRIR--
SELECT * FROM PERSONEL WHERE NOT (BOL_NO=4) --BOLUM NO 4 OLMAYANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BOL_NO IN (1,4,5) --BOLUM NO 1, 4, 5 OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BOL_NO NOT IN (1,5) --BOLUM NO 1, 5 OLMAYANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BRUT>5000 AND BRUT<10000--BRUTU 5000 BUYUK VE 10000 DEN KUCUK OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE BRUT BETWEEN 5000 AND 10000--BRUTU 5000 VE 10000 ARASI OLANLARI GETIRIR--

SELECT * FROM PERSONEL WHERE ADRES LIKE '%UL%' --UL İLE BAŞLAYANLARI GETIRIR-- 

SELECT * FROM PERSONEL WHERE ADRES LIKE '%US' --US İLE BİTENLERİ GETIRIR--

SELECT 100+20 AS SONUC --AS SUTUNA İSİM VERİR--

SELECT BOL_NO,BOL_NO*BOL_NO FROM PERSONEL --BOL_NO VE BOL_NO*BOL_NO YU GETIRIR--

SELECT SUM(BRUT) AS TOPLAM_MAAS FROM PERSONEL --PERSONEL TABLOSUNDAKI BRUT DEGERLERININ TOPLAMI--

SELECT SUM(BRUT) AS KADIN_TOPLAM_MAAS FROM PERSONEL WHERE CINSIYET='K' --PERSONEL TABLOSUNDAKI KADINLARIN BRUT DEGERLERININ TOPLAMI--

SELECT AVG(BRUT) AS ORT_MAAS FROM PERSONEL --WHERE KOMUTU ILE FILITRELENEBILIR--PERSONEL TABLOSUNDAKI BRUT DEGERLERININ ORTALAMASI--


SELECT MAX(BRUT) AS MAX_MAAS FROM PERSONEL --PERSONEL TABLOSUNDAKI BRUT DEGERLERININ EN BUYUGUNU--

SELECT MIN(BRUT) AS MIN_MAAS FROM PERSONEL --PERSONEL TABLOSUNDAKI BRUT DEGERLERININ EN KUCUGUNU--

SELECT COUNT(*) AS SATIR FROM PERSONEL --PERSONEL TABLOSUNDAKI SATIR SAYISI--

SELECT   COUNT(DISTINCT BOL_NO) AS SATIR FROM PERSONEL --PERSONEL TABLOSUNDAKİ BOLUM SAYISINI YAZAR--
